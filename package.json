{
  "name": "quorum-gossipsub",
  "version": "0.11.5",
  "description": "A froked version of gossipsub for project Quorum by @LeaskH.",
  "leadMaintainer": "Cayman Nava <caymannava@gmail.com>",
  "main": "src/index.js",
  "files": [
    "src",
    "dist"
  ],
  "types": "src/index.d.ts",
  "scripts": {
    "lint": "eslint --ext .ts ts",
    "release": "aegir release --no-types",
    "prebuild": "tsc",
    "build": "npm run build:proto && npm run build:proto-types && cp -R ts/message src && npm run build:types",
    "build:proto": "pbjs -t static-module --force-number --no-verify --no-delimited --no-create --no-beautify --no-defaults --lint eslint-disable -o ts/message/rpc.js ./ts/message/rpc.proto",
    "build:proto-types": "pbts -o ts/message/rpc.d.ts ts/message/rpc.js",
    "build:types": "aegir build --no-types",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "test": "aegir test",
    "test:node": "aegir test --target node",
    "test:browser": "aegir test --target browser",
    "updep": "npx ncu -u && npm install && ( git commit -am 'update dependencies' || true )",
    "upstream": "git pull git@github.com:ChainSafe/js-libp2p-gossipsub.git"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Leask/quorum-gossipsub.git"
  },
  "keywords": [
    "libp2p",
    "pubsub",
    "gossip"
  ],
  "author": "Cayman Nava",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Leask/quorum-gossipsub/issues"
  },
  "homepage": "https://github.com/Leask/quorum-gossipsub#readme",
  "dependencies": {
    "@types/debug": "^4.1.7",
    "debug": "^4.3.2",
    "denque": "^2.0.1",
    "err-code": "^3.0.1",
    "it-pipe": "^1.1.0",
    "libp2p-interfaces": "^1.1.1",
    "peer-id": "^0.15.3",
    "protobufjs": "^6.11.2",
    "time-cache": "^0.3.0",
    "uint8arrays": "^3.0.0"
  },
  "devDependencies": {
    "@chainsafe/libp2p-noise": "^4.1.1",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "aegir": "^35.1.1",
    "benchmark": "^2.1.4",
    "buffer": "^6.0.3",
    "chai": "^4.3.4",
    "chai-spies": "^1.0.0",
    "delay": "^5.0.0",
    "detect-node": "^2.1.0",
    "dirty-chai": "^2.0.1",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^4.1.0",
    "it-pair": "^1.0.0",
    "libp2p": "^0.33.0",
    "libp2p-floodsub": "^0.27.1",
    "libp2p-interfaces-compliance-tests": "^1.1.1",
    "libp2p-mplex": "^0.10.4",
    "libp2p-websockets": "^0.16.2",
    "lodash": "^4.17.21",
    "multiaddr": "^10.0.1",
    "os": "^0.1.2",
    "p-retry": "^4.6.1",
    "p-times": "^4.0.0",
    "p-wait-for": "^4.1.0",
    "promisify-es6": "^1.0.3",
    "sinon": "^11.1.2",
    "typescript": "4.4.x",
    "util": "^0.12.4"
  },
